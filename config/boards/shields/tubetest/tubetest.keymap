#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {  
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp K     &kp N     &kp H     &kp K
                &kp E     &kp E     &kp R     &kp A
                &kp N     &kp T     &mo 1     &kp T
            >;
        };

        bt_layer {
            bindings = <
                &none            &none            &bootloader            &none
                &none            &bt BT_PAIR            &bt BT_CLR             &none
                &bt BT_SEL 0     &bt BT_SEL 1     &bt BT_SEL 2           &none
            >;
        };
    };
};